{
  "$schema": "",
  "version": "1.0.0-*",
  "compilationOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "AspNet5Template": "1.0.0"
  },
  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel --server.urls=http://localhost:5001"
  },
  "frameworks": {
    "dnx451": {
      "dependencies": {
        "System.Linq.Expressions": "4.0.11-beta-23516",
